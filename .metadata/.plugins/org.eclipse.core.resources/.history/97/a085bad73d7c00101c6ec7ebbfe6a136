spring:
  application:
    name: OnlineBookStore-ApiGateway
  cloud:
    gateway:
      routes:
      - id: Online_Book_Store_Catalog_Service
      uri: http:localhost:8002/
      predicates:
      - Path=/catalog/**


server:
  port: 8001
eureka:
  client:
    service-url:
      defaultZone: http://localhost:8761/eureka/


  